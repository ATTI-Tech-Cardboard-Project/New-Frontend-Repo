{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    },
    {
      "env": {
      "NODE_ENV": "production"
    }
    }
  ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }
